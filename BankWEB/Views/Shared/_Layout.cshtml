<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BankWEB</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" type="text/css" media="screen" runat="server" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css">
    <link rel="prefetch" href="~/css/fonts.css" as="font" type="text/css" media="screen" runat="server">
    <link rel="stylesheet" href="~/css/style.css" type="text/css" media="screen" runat="server" asp-append-version="true">
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    @* <script asp-src-include="~/js/*.js"></script> *@
    @await RenderSectionAsync("Scripts", required: false)
</head>
<body>
    <header>
        <nav>
            @await Component.InvokeAsync("LogoutView")
        </nav>
    </header>
    <main role="main">
        @RenderBody()
    </main>

    @RenderSection("Modals", required: false)
    @* <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - BankWEB - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer> *@
     
</body>
</html>
